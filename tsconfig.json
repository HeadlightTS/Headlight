{
    "compilerOptions": {
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "target": "es5",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "module": "umd",
        "outDir": "build/Release"
    },
    "include": [
        "./src/**/*.ts"
    ]
}
